local ItemCombinations = {}

local combinations = {
	item_backdoor_key = {
		item_test = "success_test"
	}
}

function ItemCombinations.get(id_a, id_b)
	!if _ASSERT then
	Assert.must(id_a, Types.T_STRING)
	Assert.must(id_b, Types.T_STRING)
	!end

	local t = combinations[id_a]
	local t2 = t[id_b]

	return t2
end

return ItemCombinations
