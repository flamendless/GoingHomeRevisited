local Concord = require("modules.concord.concord")

Concord.component("bounding_box", function(c, x, y, w, h)
	@@assert(type(x) == "number")
	@@assert(type(y) == "number")
	@@assert(type(w) == "number")
	@@assert(type(h) == "number")
	c.x = x
	c.y = y
	c.w = w
	c.h = h
	c.screen_pos = {
		x = x,
		y = y,
	}
end)
