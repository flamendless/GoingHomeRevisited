local Concord = require("modules.concord.concord")

Concord.component("ui_element")

Concord.component("ui_text")

Concord.component("ui_action")

Concord.component("ui_offset", function(c, offset)
	!if _ASSERT then
	Assert.is(offset, Types.T_VEC2)
	!end

	c.offset = offset
end)

Concord.component("notification", function(c)
	c.is_active = false
end)
