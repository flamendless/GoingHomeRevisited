local Concord = require("modules.concord.concord")

Concord.component("typewriter_on_finish", function(c, callback, delay)
	!if _ASSERT then
	assert(type(callback) == "function", "Passed argument1 must be of type 'function'")
	if delay then
		assert(type(delay) == "number", "Passed argument2 must be of type 'number'")
	end
	!end
	c.callback = callback
	c.delay = delay or 0
end)
