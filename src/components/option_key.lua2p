local Concord = require("modules.concord.concord")

Concord.component("option_key", function(c, id, page)
	!if _ASSERT then
	assert(type(id) == "number", "Passed argument must be of type 'number'")
	assert(type(page) == "number", "Passed argument must be of type 'number'")
	!end
	c.id = id
	c.page = page
end)

Concord.component("option_disabled", function(c)
end)
